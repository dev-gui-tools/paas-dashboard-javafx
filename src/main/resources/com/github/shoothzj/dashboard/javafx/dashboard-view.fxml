<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.github.shoothzj.dashboard.javafx.DashboardController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <GridPane>
        <Button text="ZooKeeper" onAction="#onZooKeeperBtnClick" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <Button text="Kubernetes" onAction="#onKubernetesBtnClick" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <Button text="Bookkeeper" onAction="#onBookkeeperBtnClick" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    </GridPane>
</VBox>
